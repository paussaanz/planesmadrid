<div class="profile-page">
  <div class="container profile-container">
    <h1>{{currentUser.username}}'s Profile</h1>

    <img class="profile-image" src="https://picsum.photos/150" alt="Profile Image">

    <div class="text-center">
      <i class="fas fa-user-circle"></i>
    </div>


    <h2>Created Plans</h2>

    {{#each user.plans as |plan|}}
    {{>planCard this}}
    {{/each}}

    <h2>Saved Plans</h2>
  
      {{#each user.savedPlans as |plan|}}
      {{>planCard this}}
      {{/each}}
   

  </div>
</div>